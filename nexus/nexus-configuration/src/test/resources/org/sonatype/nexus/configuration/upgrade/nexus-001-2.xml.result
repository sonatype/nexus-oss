<?xml version="1.0" encoding="UTF-8"?>
<nexusConfiguration>
  <version>1.0.7</version>
  <security>
    <enabled>true</enabled>
    <anonymousAccessEnabled>true</anonymousAccessEnabled>
    <realms>
      <realm>XmlAuthenticatingRealm</realm>
      <realm>NexusMethodAuthorizingRealm</realm>
      <realm>NexusTargetAuthorizingRealm</realm>
    </realms>
  </security>
  <globalConnectionSettings />
  <globalHttpProxySettings>
    <proxyHostname>proxyhost</proxyHostname>
    <proxyPort>8080</proxyPort>
    <authentication>
      <username></username>
      <password></password>
    </authentication>
  </globalHttpProxySettings>
  <restApi />
  <httpProxy />
  <routing>
    <groups />
  </routing>
  <repositories>
    <repository>
      <id>central</id>
      <name>Maven Central</name>
      <notFoundCacheTTL>86400</notFoundCacheTTL>
      <artifactMaxAge>86400</artifactMaxAge>
      <metadataMaxAge>86400</metadataMaxAge>
      <remoteStorage>
        <url>http://repo1.maven.org/maven2/</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>sonatype</id>
      <name>Sonatype</name>
      <notFoundCacheTTL>186400</notFoundCacheTTL>
      <artifactMaxAge>86400</artifactMaxAge>
      <metadataMaxAge>86400</metadataMaxAge>
      <remoteStorage>
        <url>http://hehe.sonatype.com/repos/customer</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>freehep</id>
      <name>Freehep</name>
      <notFoundCacheTTL>86400</notFoundCacheTTL>
      <artifactMaxAge>86400</artifactMaxAge>
      <metadataMaxAge>86400</metadataMaxAge>
      <remoteStorage>
        <url>http://java.freehep.org/maven2/</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>apache.incubator</id>
      <name>Apache Inbubator</name>
      <notFoundCacheTTL>86400</notFoundCacheTTL>
      <artifactMaxAge>86400</artifactMaxAge>
      <metadataMaxAge>86400</metadataMaxAge>
      <remoteStorage>
        <url>http://people.apache.org/repo/m2-incubating-repository/</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>atlassian</id>
      <name>Atlassian</name>
      <notFoundCacheTTL>86400</notFoundCacheTTL>
      <artifactMaxAge>86400</artifactMaxAge>
      <metadataMaxAge>86400</metadataMaxAge>
      <remoteStorage>
        <url>https://maven.atlassian.com/public/</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>openfast</id>
      <name>OpenFAST</name>
      <notFoundCacheTTL>86400</notFoundCacheTTL>
      <artifactMaxAge>86400</artifactMaxAge>
      <metadataMaxAge>86400</metadataMaxAge>
      <remoteStorage>
        <url>http://www.openfast.org/maven/release/</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>codehaus.snapshot</id>
      <name>Codehaus Snapshots</name>
      <notFoundCacheTTL>86400</notFoundCacheTTL>
      <artifactMaxAge>86400</artifactMaxAge>
      <metadataMaxAge>86400</metadataMaxAge>
      <repositoryPolicy>snapshot</repositoryPolicy>
      <remoteStorage>
        <url>http://snapshots.repository.codehaus.org/</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>apache.snapshot</id>
      <name>Apache Snapshots</name>
      <notFoundCacheTTL>6400</notFoundCacheTTL>
      <artifactMaxAge>60</artifactMaxAge>
      <metadataMaxAge>60</metadataMaxAge>
      <repositoryPolicy>snapshot</repositoryPolicy>
      <remoteStorage>
        <url>http://people.apache.org/repo/m2-snapshot-repository/</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>freehep.snapshot</id>
      <name>Freehep Snapshots</name>
      <notFoundCacheTTL>86400</notFoundCacheTTL>
      <artifactMaxAge>86400</artifactMaxAge>
      <metadataMaxAge>86400</metadataMaxAge>
      <repositoryPolicy>snapshot</repositoryPolicy>
      <remoteStorage>
        <url>http://java.freehep.org/maven2/</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>atlassian.snapshot</id>
      <name>Atlassian Snapshots</name>
      <notFoundCacheTTL>86400</notFoundCacheTTL>
      <artifactMaxAge>86400</artifactMaxAge>
      <metadataMaxAge>86400</metadataMaxAge>
      <repositoryPolicy>snapshot</repositoryPolicy>
      <remoteStorage>
        <url>https://maven.atlassian.com/public-snapshot/</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>openfast.snapshot</id>
      <name>Openfast Snapshots</name>
      <notFoundCacheTTL>86400</notFoundCacheTTL>
      <artifactMaxAge>86400</artifactMaxAge>
      <metadataMaxAge>86400</metadataMaxAge>
      <repositoryPolicy>snapshot</repositoryPolicy>
      <remoteStorage>
        <url>http://www.openfast.org/maven/snapshot/</url>
      </remoteStorage>
    </repository>
  </repositories>
  <repositoryGrouping>
    <pathMappings>
      <pathMapping>
        <id>migrated-I1</id>
        <groupId>*</groupId>
        <routePattern>.*/org/apache/.*</routePattern>
        <routeType>inclusive</routeType>
        <repositories>
          <repository>central</repository>
          <repository>sonatype</repository>
          <repository>apache.snapshot</repository>
          <repository>apache.incubator</repository>
        </repositories>
      </pathMapping>
      <pathMapping>
        <id>migrated-I2</id>
        <groupId>*</groupId>
        <routePattern>.*/org/codehaus/.*</routePattern>
        <routeType>inclusive</routeType>
        <repositories>
          <repository>central</repository>
          <repository>codehaus.snapshot</repository>
          <repository>sonatype</repository>
        </repositories>
      </pathMapping>
      <pathMapping>
        <id>migrated-E1</id>
        <groupId>*</groupId>
        <routePattern>.*/com/dec/.*</routePattern>
        <routeType>exclusive</routeType>
        <repositories>
          <repository>sonatype</repository>
          <repository>apache.incubator</repository>
          <repository>central</repository>
          <repository>freehep</repository>
          <repository>atlassian</repository>
          <repository>apache.snapshot</repository>
          <repository>codehaus.snapshot</repository>
          <repository>apache.snapshot</repository>
          <repository>freehep.snapshot</repository>
          <repository>atlassian.snapshot</repository>
        </repositories>
      </pathMapping>
    </pathMappings>
    <repositoryGroups>
      <repositoryGroup>
        <groupId>public</groupId>
        <repositories>
          <repository>central</repository>
          <repository>sonatype</repository>
          <repository>freehep</repository>
          <repository>atlassian</repository>
          <repository>apache.incubator</repository>
          <repository>openfast</repository>
        </repositories>
      </repositoryGroup>
      <repositoryGroup>
        <groupId>snapshot</groupId>
        <repositories>
          <repository>apache.snapshot</repository>
          <repository>codehaus.snapshot</repository>
          <repository>freehep.snapshot</repository>
          <repository>atlassian.snapshot</repository>
          <repository>openfast.snapshot</repository>
        </repositories>
      </repositoryGroup>
    </repositoryGroups>
  </repositoryGrouping>
  <repositoryTargets>
    <repositoryTarget>
      <id>1</id>
      <name>All (Maven2)</name>
      <contentClass>maven2</contentClass>
      <patterns>
        <pattern>.*</pattern>
      </patterns>
    </repositoryTarget>
    <repositoryTarget>
      <id>2</id>
      <name>All (Maven1)</name>
      <contentClass>maven1</contentClass>
      <patterns>
        <pattern>.*</pattern>
      </patterns>
    </repositoryTarget>
    <repositoryTarget>
      <id>3</id>
      <name>All but sources (Maven2)</name>
      <contentClass>maven2</contentClass>
      <patterns>
        <pattern>(?!.*-sources.*).*</pattern>
      </patterns>
    </repositoryTarget>
    <repositoryTarget>
      <id>4</id>
      <name>All Metadata (Maven2)</name>
      <contentClass>maven2</contentClass>
      <patterns>
        <pattern>.*maven-metadata\.xml.*</pattern>
      </patterns>
    </repositoryTarget>
  </repositoryTargets>
  <smtpConfiguration>
    <host>smtp-host</host>
    <port>25</port>
    <username>smtp-username</username>
    <password>smtp-password</password>
    <systemEmailAddress>system@nexus.org</systemEmailAddress>
  </smtpConfiguration>
</nexusConfiguration>
