<?xml version="1.0" encoding="UTF-8"?>
<nexusConfiguration>
  <version>1.0.8</version>
  <security>
    <enabled>true</enabled>
    <anonymousAccessEnabled>true</anonymousAccessEnabled>
    <anonymousUsername>anonymous2</anonymousUsername>
    <anonymousPassword>anonymous2</anonymousPassword>
    <realms>
      <realm>XmlAuthenticatingRealm</realm>
      <realm>XmlAuthorizingRealm</realm>
    </realms>
  </security>
  <globalConnectionSettings />
  <restApi />
  <httpProxy />
  <routing>
    <groups />
  </routing>
  <repositories>
    <repository>
      <id>sonatype-releases</id>
      <name>Sonatype Releases</name>
      <allowWrite>true</allowWrite>
      <remoteStorage>
        <url>http://svn.sonatype.com/mvnrepos/releases</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>sonatype-snapshots</id>
      <name>Sonatype Snapshots</name>
      <allowWrite>true</allowWrite>
      <repositoryPolicy>snapshot</repositoryPolicy>
      <remoteStorage>
        <url>http://svn.sonatype.com/mvnrepos/snapshots</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>sonatype-customer</id>
      <name>Sonatype Customer</name>
      <allowWrite>true</allowWrite>
      <remoteStorage>
        <url>http://svn.sonatype.com/mvnrepos/customer</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>central</id>
      <name>Maven Central</name>
      <allowWrite>true</allowWrite>
      <remoteStorage>
        <url>http://repo1.maven.org/maven2/</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>codehaus</id>
      <name>Codehaus</name>
      <allowWrite>true</allowWrite>
      <remoteStorage>
        <url>http://repository.codehaus.org/</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>dev-java-net-releases</id>
      <name>dev.java.net</name>
      <allowWrite>true</allowWrite>
      <remoteStorage>
        <url>http://download.java.net/maven/2/</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>restlet-org-releases</id>
      <name>Restlet.org</name>
      <allowWrite>true</allowWrite>
      <remoteStorage>
        <url>http://maven.restlet.org/</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>google-releases</id>
      <name>Google GWT</name>
      <allowWrite>true</allowWrite>
      <remoteStorage>
        <url>http://gwt-maven.googlecode.com/svn/trunk/mavenrepo/</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>apache-snapshots</id>
      <name>Apache Snapshots</name>
      <allowWrite>true</allowWrite>
      <repositoryPolicy>snapshot</repositoryPolicy>
      <remoteStorage>
        <url>http://people.apache.org/repo/m2-snapshot-repository</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>codehaus-snapshots</id>
      <name>Codehaus Snapshots</name>
      <allowWrite>true</allowWrite>
      <repositoryPolicy>snapshot</repositoryPolicy>
      <remoteStorage>
        <url>http://snapshots.repository.codehaus.org/</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>restlet-org-snapshots</id>
      <name>Restlet.org</name>
      <allowWrite>true</allowWrite>
      <repositoryPolicy>snapshot</repositoryPolicy>
      <remoteStorage>
        <url>http://maven.restlet.org/</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>inhouse</id>
      <name>Releases</name>
      <allowWrite>true</allowWrite>
    </repository>
    <repository>
      <id>inhouse-snapshots</id>
      <name>Snapshots</name>
      <allowWrite>true</allowWrite>
      <repositoryPolicy>snapshot</repositoryPolicy>
    </repository>
    <repository>
      <id>extFree</id>
      <name>Modified OSS</name>
      <allowWrite>true</allowWrite>
    </repository>
    <repository>
      <id>extNonFree</id>
      <name>Commerical</name>
      <allowWrite>true</allowWrite>
    </repository>
  </repositories>
  <repositoryShadows>
    <repositoryShadow>
      <id>central-m1</id>
      <name>Central M1 shadow</name>
      <shadowOf>central</shadowOf>
      <type>m2-m1-shadow</type>
    </repositoryShadow>
    <repositoryShadow>
      <id>codehaus-m1</id>
      <name>Codehaus M1 shadow</name>
      <shadowOf>codehaus</shadowOf>
      <type>m2-m1-shadow</type>
    </repositoryShadow>
  </repositoryShadows>
  <repositoryGrouping>
    <pathMappings>
      <pathMapping>
        <id>inhouse-stuff</id>
        <groupId>*</groupId>
        <routePattern>.*/(com|org)/sonatype/.*</routePattern>
        <routeType>inclusive</routeType>
        <repositories>
          <repository>sonatype-snapshots</repository>
          <repository>sonatype-releases</repository>
          <repository>sonatype-customer</repository>
        </repositories>
      </pathMapping>
      <pathMapping>
        <id>maven-stuff</id>
        <groupId>*</groupId>
        <routePattern>.*/org/apache/maven/.*</routePattern>
        <routeType>inclusive</routeType>
        <repositories>
          <repository>apache-snapshots</repository>
          <repository>central</repository>
          <repository>sonatype-snapshots</repository>
          <repository>sonatype-releases</repository>
          <repository>sonatype-customer</repository>
        </repositories>
      </pathMapping>
      <pathMapping>
        <id>plexus-stuff</id>
        <groupId>*</groupId>
        <routePattern>.*/org/codehaus/plexus/.*</routePattern>
        <routeType>inclusive</routeType>
        <repositories>
          <repository>codehaus-snapshots</repository>
          <repository>central</repository>
          <repository>sonatype-snapshots</repository>
          <repository>sonatype-releases</repository>
          <repository>sonatype-customer</repository>
        </repositories>
      </pathMapping>
      <pathMapping>
        <id>apache-stuff</id>
        <groupId>*</groupId>
        <routePattern>.*/org/apache/.*</routePattern>
        <routeType>exclusive</routeType>
        <repositories>
          <repository>inhouse</repository>
          <repository>inhouse-snapshots</repository>
        </repositories>
      </pathMapping>
    </pathMappings>
    <repositoryGroups>
      <repositoryGroup>
        <groupId>sonatype-nexus</groupId>
        <type>maven2</type>
        <repositories>
          <repository>sonatype-snapshots</repository>
          <repository>sonatype-releases</repository>
          <repository>sonatype-customer</repository>
          <repository>central</repository>
          <repository>apache-snapshots</repository>
          <repository>codehaus-snapshots</repository>
          <repository>restlet-org-releases</repository>
          <repository>dev-java-net-releases</repository>
          <repository>google-releases</repository>
        </repositories>
      </repositoryGroup>
      <repositoryGroup>
        <groupId>public-releases</groupId>
        <type>maven2</type>
        <repositories>
          <repository>extFree</repository>
          <repository>extNonFree</repository>
          <repository>central</repository>
          <repository>codehaus</repository>
          <repository>dev-java-net-releases</repository>
        </repositories>
      </repositoryGroup>
      <repositoryGroup>
        <groupId>public-snapshots</groupId>
        <type>maven2</type>
        <repositories>
          <repository>apache-snapshots</repository>
          <repository>codehaus-snapshots</repository>
        </repositories>
      </repositoryGroup>
      <repositoryGroup>
        <groupId>inhouse-releases</groupId>
        <type>maven2</type>
        <repositories>
          <repository>inhouse</repository>
        </repositories>
      </repositoryGroup>
      <repositoryGroup>
        <groupId>inhouse-snapshots</groupId>
        <type>maven2</type>
        <repositories>
          <repository>inhouse-snapshots</repository>
        </repositories>
      </repositoryGroup>
    </repositoryGroups>
  </repositoryGrouping>
  <repositoryTargets>
    <repositoryTarget>
      <id>1</id>
      <name>All (Maven2)</name>
      <contentClass>maven2</contentClass>
      <patterns>
        <pattern>.*</pattern>
      </patterns>
    </repositoryTarget>
    <repositoryTarget>
      <id>2</id>
      <name>All (Maven1)</name>
      <contentClass>maven1</contentClass>
      <patterns>
        <pattern>.*</pattern>
      </patterns>
    </repositoryTarget>
    <repositoryTarget>
      <id>3</id>
      <name>All but sources (Maven2)</name>
      <contentClass>maven2</contentClass>
      <patterns>
        <pattern>(?!.*-sources.*).*</pattern>
      </patterns>
    </repositoryTarget>
    <repositoryTarget>
      <id>4</id>
      <name>All Metadata (Maven2)</name>
      <contentClass>maven2</contentClass>
      <patterns>
        <pattern>.*maven-metadata\.xml.*</pattern>
      </patterns>
    </repositoryTarget>
  </repositoryTargets>
  <tasks>
    <task>
      <id>1</id>
      <name>weekly_task</name>
      <enabled>true</enabled>
      <type>ReindexTask</type>
      <status>SUBMITTED</status>
      <nextRun>1229291340000</nextRun>
      <schedule>
        <type>weekly</type>
        <startDate>1229032140000</startDate>
        <daysOfWeek>
          <daysOfWeek>1</daysOfWeek>
          <daysOfWeek>2</daysOfWeek>
        </daysOfWeek>
      </schedule>
      <properties>
        <property>
          <key>repositoryOrGroupId</key>
          <value>all_repo</value>
        </property>
      </properties>
    </task>
  </tasks>
  <smtpConfiguration>
    <host>smtp-host</host>
    <port>25</port>
    <username>smtp-username</username>
    <password>smtp-password</password>
    <systemEmailAddress>system@nexus.org</systemEmailAddress>
  </smtpConfiguration>
</nexusConfiguration>
