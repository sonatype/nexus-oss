<?xml version="1.0" encoding="UTF-8"?>
<nexusConfiguration>
  <version>1.0.4</version>
  <workingDirectory>/dat/maven/mvnrepos</workingDirectory>
  <applicationLogDirectory>${basedir}/logs/</applicationLogDirectory>
  <security>
    <enabled>true</enabled>
    <anonymousAccessEnabled>true</anonymousAccessEnabled>
    <configurationFile>${runtime}/apps/nexus/conf/security.xml</configurationFile>
    <realms>
      <realm>default</realm>
    </realms>
  </security>
  <globalConnectionSettings />
  <globalHttpProxySettings>
    <proxyHostname>fproxyint0t</proxyHostname>
    <proxyPort>8080</proxyPort>
  </globalHttpProxySettings>
  <restApi />
  <httpProxy />
  <routing>
    <groups />
  </routing>
  <repositories>
    <repository>
      <id>central</id>
      <name>Maven Central</name>
      <notFoundCacheTTL>86400</notFoundCacheTTL>
      <artifactMaxAge>518400</artifactMaxAge>
      <metadataMaxAge>518400</metadataMaxAge>
      <remoteStorage>
        <url>http://192.168.228.40:8443/nexus/groups/public</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>central.snapshot</id>
      <name>Maven Central Snapshots</name>
      <notFoundCacheTTL>46400</notFoundCacheTTL>
      <artifactMaxAge>60</artifactMaxAge>
      <metadataMaxAge>60</metadataMaxAge>
      <repositoryPolicy>snapshot</repositoryPolicy>
      <remoteStorage>
        <url>http://192.168.228.40:8443/nexus/groups/snapshot</url>
      </remoteStorage>
    </repository>
    <repository>
      <id>release</id>
      <name>Release</name>
      <allowWrite>true</allowWrite>
      <notFoundCacheTTL>86400</notFoundCacheTTL>
      <artifactMaxAge>86400</artifactMaxAge>
      <metadataMaxAge>86400</metadataMaxAge>
    </repository>
    <repository>
      <id>commercial</id>
      <name>Commercial Software</name>
      <allowWrite>true</allowWrite>
      <notFoundCacheTTL>86400</notFoundCacheTTL>
      <artifactMaxAge>86400</artifactMaxAge>
      <metadataMaxAge>86400</metadataMaxAge>
    </repository>
    <repository>
      <id>team</id>
      <name>Team</name>
      <allowWrite>true</allowWrite>
      <notFoundCacheTTL>86400</notFoundCacheTTL>
      <artifactMaxAge>86400</artifactMaxAge>
      <metadataMaxAge>86400</metadataMaxAge>
    </repository>
    <repository>
      <id>snapshot</id>
      <name>Snapshots</name>
      <allowWrite>true</allowWrite>
      <notFoundCacheTTL>86400</notFoundCacheTTL>
      <artifactMaxAge>86400</artifactMaxAge>
      <metadataMaxAge>86400</metadataMaxAge>
      <repositoryPolicy>snapshot</repositoryPolicy>
    </repository>
    <repository>
      <id>opensource</id>
      <name>Modified OSS</name>
      <allowWrite>true</allowWrite>
      <notFoundCacheTTL>86400</notFoundCacheTTL>
      <artifactMaxAge>86400</artifactMaxAge>
      <metadataMaxAge>86400</metadataMaxAge>
    </repository>
  </repositories>
  <repositoryGrouping>
    <pathMappings>
      <pathMapping>
        <id>migrated-E1</id>
        <groupId>*</groupId>
        <routePattern>.*/com/abc/.*</routePattern>
        <routeType>exclusive</routeType>
        <repositories>
          <repository>central</repository>
          <repository>central.snapshot</repository>
        </repositories>
      </pathMapping>
      <pathMapping>
        <id>migrated-E2</id>
        <groupId>*</groupId>
        <routePattern>.*/net/transacttools/.*</routePattern>
        <routeType>exclusive</routeType>
        <repositories>
          <repository>central</repository>
          <repository>central.snapshot</repository>
        </repositories>
      </pathMapping>
    </pathMappings>
    <repositoryGroups>
      <repositoryGroup>
        <groupId>public</groupId>
        <repositories>
          <repository>release</repository>
          <repository>team</repository>
          <repository>commercial</repository>
          <repository>opensource</repository>
          <repository>central</repository>
          <repository>snapshot</repository>
        </repositories>
      </repositoryGroup>
      <repositoryGroup>
        <groupId>public.snapshot</groupId>
        <repositories>
          <repository>central.snapshot</repository>
        </repositories>
      </repositoryGroup>
    </repositoryGroups>
  </repositoryGrouping>
  <smtpConfiguration>
    <host>smtp-host</host>
    <port>1234</port>
    <username>smtp-username</username>
    <password>smtp-password</password>
    <systemEmailAddress>system@nexus.org</systemEmailAddress>
  </smtpConfiguration>
</nexusConfiguration>
